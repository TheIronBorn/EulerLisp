; Tail recursive, but reverses the array,
; in this case, that is no problem
(defn delete-nth_ (n lst (acc '()))
  (if (= n 0)
      (cons
        (fst (fst lst))
        (append (map rst (rst lst)) acc))
      (delete-nth_ (dec n) (rst lst)
                  (cons (rst (fst lst)) acc)
                  )))

(defn matrix-sum (m)
  (if (nil? m)
      0
      (~>
        (range~ 0 (dec (length m)))
        (map~ (fn (i)
                (let ([del (delete-nth_ i m)])
                  (+ (fst del)
                     (matrix-sum (rst del))))))
        (reduce~ max 0))))



